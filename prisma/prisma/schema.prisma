datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

generator photonjs {
  provider = "photonjs"
}

generator nexus_prisma {
  provider = "nexus-prisma"
}

model User {
  id                   String            @default(cuid()) @id @unique
  email                String            @unique
  password             String
}
